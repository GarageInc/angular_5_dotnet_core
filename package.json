{
    "name": "depot-app",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "build": "ng build --prod",
        "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
        "serve:search": "node wwwroot/server.js",
        "start:search": "ng serve --app=1 --deploy-url=search_module --proxy-config proxy.config.json --port 4201",
        "start:admin": "ng serve --app=0 --deploy-url=admin_module --proxy-config proxy.config.json --port 4201",
        "start:lc": "ng serve --app=3 --deploy-url=lc_module --proxy-config proxy.config.json --port 4201",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "deploy": "path=$(pwd) && sudo -Hu deploy sh -c \"cd $path && npm run deploy:git && npm run deploy:npm_install && npm run deploy:build_all && npm run deploy:currencies\"",
        "deploy:build_all": "npm run deploy:build_search && npm run deploy:build_admin && npm run deploy:build_lc",
        "deploy:git": "npm run deploy:git_reset && npm run deploy:git_pull",
        "deploy:git_reset": "git reset --hard",
        "deploy:git_pull": "git pull",
        "deploy:npm_install": "npm install",
        "deploy:migrations": "",
        "deploy:flush_cache": "",
        "deploy:after_deploy": "",
        "deploy:build_search": "ng build --prod --app 1 && ng build --prod --app 2 --output-hashing=false && npm run webpack:server",
        "deploy:build_admin": "ng build --app=0 --prod  --build-optimizer  --deploy-url=admin_module",
        "deploy:build_lc": "ng build --app=3 --prod --build-optimizer   --deploy-url=lc_module",
        "deploy:uglify_search": "distPath=$(pwd)/wwwroot/search_module && uglifyes='node_modules/uglify-es/bin/uglifyjs' && $uglifyes --output $distPath/main.bundle.js $distPath/main.bundle.js && $uglifyes --output $distPath/inline.bundle.js $distPath/inline.bundle.js && $uglifyes --output $distPath/polyfills.bundle.js $distPath/polyfills.bundle.js && $uglifyes --output $distPath/vendor.bundle.js $distPath/vendor.bundle.js && $uglifyes --output $distPath/scripts.bundle.js $distPath/scripts.bundle.js",
        "deploy:uglify_admin": "distPath=$(pwd)/wwwroot/admin_module && uglifyes='node_modules/uglify-es/bin/uglifyjs' && $uglifyes --output $distPath/main.bundle.js $distPath/main.bundle.js && $uglifyes --output $distPath/inline.bundle.js $distPath/inline.bundle.js && $uglifyes --output $distPath/polyfills.bundle.js $distPath/polyfills.bundle.js && $uglifyes --output $distPath/vendor.bundle.js $distPath/vendor.bundle.js ",
        "deploy:permissions": "group=$(id -nu 33) && path=$(pwd) && sudo /bin/chmod -R 775 $path'/src' && sudo chown -R deploy:$group $path'/src'",
        "deploy:permissions_node_modules": "group=$(id -nu 33) && path=$(pwd) && sudo /bin/chmod -R 775 $path'/node_modules' && sudo chown -R deploy:$group $path'/node_modules'",
        "deploy:backend": "sudo -Hu deploy sh -c \"sudo systemctl stop kestrel-depot.service && dotnet ef database update && dotnet publish && sudo systemctl start kestrel-depot.service\"",
        "deploy:currencies": "sudo /etc/cron.hourly/cbr"
    },
    "private": true,
